<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="vehicle_id" default="default"/>

  <let name="sensor_model" value="aip_customized"/>
  <let name="parent_frame" value="base_link"/>

  <!-- extrinsic_calibration_client -->
  <arg name="src_yaml" default="$(find-pkg-share individual_params)/config/$(var vehicle_id)/$(var sensor_model)/sensors_calibration.yaml"/>
  <arg name="dst_yaml" default="$(env HOME)/sensors_calibration.yaml"/>

  <!--node pkg="extrinsic_calibration_client" exec="extrinsic_calibration_client" name="extrinsic_calibration_client" output="screen">
    <param name="src_path" value="$(var src_yaml)"/>
    <param name="dst_path" value="$(var dst_yaml)"/>
  </node-->

  <!-- extrinsic_calibration_manager -->
  <!--node pkg="extrinsic_calibration_manager" exec="extrinsic_calibration_manager" name="extrinsic_calibration_manager" output="screen">
    <param name="parent_frame" value="$(var parent_frame)"/>
    <param name="child_frames" value="
    [sensor_kit_base_link,
    ars408_front_center,
    ars408_front_left,
    ars408_front_right]"/>
  </node-->

  <!-- extrinsic_manual_calibrator -->
  <!--include file="$(find-pkg-share extrinsic_manual_calibrator)/launch/calibrator.launch.xml">
    <arg name="ns" value="$(var parent_frame)/sensor_kit_base_link"/>
    <arg name="parent_frame" value="$(var parent_frame)"/>
    <arg name="child_frame" value="sensor_kit_base_link"/>
  </include>

  <include file="$(find-pkg-share extrinsic_manual_calibrator)/launch/calibrator.launch.xml">
    <arg name="ns" value="$(var parent_frame)/ars408_front_center"/>
    <arg name="parent_frame" value="$(var parent_frame)"/>
    <arg name="child_frame" value="ars408_front_center"/>
  </include>

  <include file="$(find-pkg-share extrinsic_manual_calibrator)/launch/calibrator.launch.xml">
    <arg name="ns" value="$(var parent_frame)/ars408_front_left"/>
    <arg name="parent_frame" value="$(var parent_frame)"/>
    <arg name="child_frame" value="ars408_front_left"/>
  </include>

  <include file="$(find-pkg-share extrinsic_manual_calibrator)/launch/calibrator.launch.xml">
    <arg name="ns" value="$(var parent_frame)/ars408_front_right"/>
    <arg name="parent_frame" value="$(var parent_frame)"/>
    <arg name="child_frame" value="ars408_front_right"/>
  </include>

  <include file="$(find-pkg-share extrinsic_manual_calibrator)/launch/calibrator.launch.xml">
    <arg name="ns" value="$(var parent_frame)/ars408_rear_center"/>
    <arg name="parent_frame" value="$(var parent_frame)"/>
    <arg name="child_frame" value="ars408_rear_center"/>
  </include>

  <include file="$(find-pkg-share extrinsic_manual_calibrator)/launch/calibrator.launch.xml">
    <arg name="ns" value="$(var parent_frame)/ars408_rear_left"/>
    <arg name="parent_frame" value="$(var parent_frame)"/>
    <arg name="child_frame" value="ars408_rear_left"/>
  </include>

  <include file="$(find-pkg-share extrinsic_manual_calibrator)/launch/calibrator.launch.xml">
    <arg name="ns" value="$(var parent_frame)/ars408_rear_right"/>
    <arg name="parent_frame" value="$(var parent_frame)"/>
    <arg name="child_frame" value="ars408_rear_right"/>
  </include-->
</launch>
